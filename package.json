{
  "name": "@dotsunited/load-css-polyfill",
  "version": "1.2.0",
  "description": "A simple polyfill for asynchronously loading non-critical CSS using link[rel=preload].",
  "author": "Dots United GmbH",
  "license": "MIT",
  "main": "dist/load-css-polyfill.js",
  "module": "dist/load-css-polyfill.mjs",
  "files": [
